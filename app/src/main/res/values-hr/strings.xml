    <!-- Licenses -->
    <string name="licenses">Licence</string>
    <string name="skyline_license_description">Skyline je licenciran pod Mozilla Public License 2.0</string>
    <string name="ryujinx_description">Ryujinx je korišten kao referenca tijekom projekta zbog njihovih izuzetno preciznih implementacija\n\n<b>Napomena: Skyline nije temeljen na Ryujinx-u</b></string>
    <string name="shader_compiler_description">Skyline-ov kompilator sjena je <b>fork</b> od kompilatora sjena <i>yuzu</i> s Skyline-specifičnim promjenama, dok su druge promjene dijeljene između njih, što omogućava oba projekta da imaju koristi od njegovog razvoja\n\n<b>Napomena:</b> Korištenje u Skyline-u je moguće samo zbog izuzeća od licenciranja koje je odobrila ekipa <i>yuzu</i></string>
    <string name="sirit_description">Skyline-ov shader kompilator internom koristi Sirit za učinkovito izlučivanje SPIR-V</string>
    <string name="vkhpp_description">Vulkan-Hpp pruža C++ API za Vulkan</string>
    <string name="vkma_description">Vulkan Memory Allocator pruža visokokvalitetni alocator memorije za GPU s podrškom za UMA</string>
    <string name="khvkval_description">Khronos-ov Vulkan Validation Layer pruža provjeru grešaka za pozive Vulkan API-ja u debug izdanjima</string>
    <string name="oboe_description">Oboe se koristi za izlaz visokih performansi zvuka</string>
    <string name="perfetto_description">Perfetto se koristi za profiliranje i otklanjanje problema s detaljnim praćenjem</string>
    <string name="fmtlib_description">{fmt} pruža moderni C++ API za formatiranje API</string>
    <string name="boost_description">Boost pruža visokokvalitetne implementacije kontejnera i drugih korisnih funkcija u C++</string>
    <string name="rangev3_description">Range v3 je referentna implementacija ISO C++ 20 raspona, što nam omogućava korištenje raspona dok se razvijaju u LLVM-ovom libc++</string>
    <string name="frozen_description">Frozen pruža neizmjenjive hash mape na razini kompilacije s savršenom hash funkcijom za brže pretraživanje</string>
    <string name="pugixml_description">pugixml se koristi za parsiranje XML postavki kako bi se dohvaćale postavke iz C++</string>
    <string name="opus_description">Opus Audio Codec pruža visokokvalitetni Opus dekoder koji se koristi za dekodiranje Opus-kodiranog zvuka koji pruža gost</string>
    <string name="mbedtls_description">Mbed TLS se koristi za dekripciju AES-kodiranih NCAs</string>
    <string name="lz4_description">LZ4 se koristi za izvlačenje LZ4-komprimiranih sekcija ELF-a iz NSO-a</string>
    <string name="tzcode_description">tzcode pruža C API za pristup tzdb-u za podatke o vremenskim zonama</string>
    <string name="jssim_description">Java String Similarity omogućava našu funkcionalnost pretraživanja</string>
    <string name="andx_description">AndroidX pruža dosljedno korisničko iskustvo na svim verzijama Androida</string>
    <string name="amat_description">Android Material Components pružaju dosljedno korisničko sučelje u skladu s Material Design-om</string>
    <string name="ktstd_description">Kotlin Standard Library pruža korisne funkcije u Kotlinu</string>
